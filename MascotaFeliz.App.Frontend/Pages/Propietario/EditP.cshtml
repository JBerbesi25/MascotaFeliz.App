@page
@model MascotaFeliz.App.Frontend.Pages.EditPModelP
@{
    int propietarioId=Model.Propietarios.Id;
}
<h2>Edicion de los Propietarios: @Model.Propietarios.Nombres</h2>
<form method="post">
    <input type="hidden" asp-for="Propietarios.Id"/>
    <div class="form-group">
        <label asp-for="Propietarios.Identificacion"></label>
        <input asp-for="Propietarios.Identificacion" class ="form-control">
        <span class="textdanger" asp-validation-for="Propietarios.Identificacion"></span>
    </div>
    <div class="form-group">
        <label asp-for="Propietarios.Nombres"></label>
        <input asp-for="Propietarios.Nombres" class ="form-control">
        <span class="textdanger" asp-validation-for="Propietarios.Nombres"></span>
    </div>
    <div class="form-group">
        <label asp-for="Propietarios.Apellidos"></label>
        <input asp-for="Propietarios.Apellidos" class ="form-control">
        <span class="textdanger" asp-validation-for="Propietarios.Apellidos"></span>
    </div>
    <div class="form-group">
        <label asp-for="Propietarios.Telefono"></label>
        <input asp-for="Propietarios.Telefono" class ="form-control">
        <span class="textdanger" asp-validation-for="Propietarios.Telefono"></span>
    </div>
    <div class="form-group">
        <label asp-for="Propietarios.Direccion"></label>
        <input asp-for="Propietarios.Direccion" class ="form-control">
        <span class="textdanger" asp-validation-for="Propietarios.Direccion"></span>
    </div>
    <button type="submit" class="btn btn-primary">Guardar</button>
    </form>
<a asp-page="./ListP" class="btn-default">Lista de Propietarios</a>
