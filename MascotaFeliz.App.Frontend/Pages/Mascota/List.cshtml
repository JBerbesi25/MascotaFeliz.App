@page
@model MascotaFeliz.App.Frontend.Pages.ListModel
@{
}
<h1>Mascotas Afiliadas</h1>
<table class="table">
    @foreach (var mascota in Model.Mascotas)
    {
        <tr>
            <td>
                @mascota.Nombre
            </td>
             <td>
                @mascota.Propietario
            </td>
             <td>
                @mascota.Especie
            </td>
            <td>
                @mascota.Raza
            </td>
        </tr>
    }

</table>